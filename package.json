{
  "name": "job-dorker",
  "version": "1.0.0",
  "description": "Node.js Job Scraper using Google Dorks with intelligent parsing and comprehensive reporting",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsup",
    "start": "node dist/index.js",
    "test": "vitest",
    "test:unit": "vitest run --reporter=verbose tests/unit",
    "test:integration": "vitest run --reporter=verbose tests/integration",
    "test:security": "vitest run --reporter=verbose tests/security",
    "test:e2e": "playwright test",
    "test:selenium": "node tests/selenium/selenium-test-runner.js",
    "test:selenium:export": "node tests/selenium/export-sharing-selenium.test.js",
    "test:selenium:webapp": "node tests/selenium/web-app-selenium.test.js",
    "start:web": "tsx src/web/server-cli.ts",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest --watch",
    "test:cli": "tsx scripts/test-cli-automation.ts",
    "test:cli-simple": "tsx scripts/test-cli-simple.ts",
    "lint": "biome check .",
    "lint:fix": "biome check --apply .",
    "format": "biome format --write .",
    "format:check": "biome format --write .",
    "type-check": "tsc --noEmit",
    "typecheck": "tsc --noEmit",
    "clean": "rimraf dist",
    "prepare": "husky install",
    "prebuild": "npm run clean && npm run typecheck",
    "profile": "clinic doctor -- node dist/index.js",
    "benchmark": "autocannon -c 10 -d 30 http://localhost:3000",
    "migrate": "tsx scripts/migrate.ts",
    "seed": "tsx scripts/seed.ts",
    "web": "tsx src/web/cli.ts start",
    "web:dev": "tsx watch src/web/cli.ts start --log-level debug",
    "web:prod": "node dist/web/cli.js start",
    "dev:web": "tsx watch src/web/cli.ts start --host 0.0.0.0 --port 3001 --log-level debug",
    "test:performance": "node tests/performance/performance-test-suite.js standard",
    "test:performance:quick": "node tests/performance/performance-test-suite.js quick",
    "test:performance:standard": "node tests/performance/performance-test-suite.js standard",
    "test:performance:comprehensive": "node tests/performance/performance-test-suite.js comprehensive",
    "test:benchmark": "node tests/performance/benchmark-stress-tests.js",
    "test:stress": "node tests/performance/benchmark-stress-tests.js",
    "test:load": "node tests/performance/load-endurance-tests.js",
    "test:endurance": "node tests/performance/load-endurance-tests.js",
    "monitor:resources": "node tests/performance/resource-monitor.js",
    "reports:performance": "python3 -m http.server 8080 --directory tests/performance/reports"
  },
  "keywords": [
    "job-scraper",
    "google-dorks",
    "web-scraping",
    "job-search",
    "nodejs",
    "typescript",
    "playwright",
    "cli"
  ],
  "author": "Your Name",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@fastify/cors": "^10.1.0",
    "@fastify/rate-limit": "^10.3.0",
    "@fastify/sensible": "^6.0.3",
    "@fastify/static": "^8.2.0",
    "@fastify/view": "^9.1.0",
    "@fastify/websocket": "^11.2.0",
    "better-sqlite3": "^9.2.2",
    "bottleneck": "^2.19.5",
    "bullmq": "^5.1.5",
    "chalk": "^5.3.0",
    "chart.js": "^4.5.0",
    "cheerio": "^1.0.0-rc.12",
    "commander": "^11.1.0",
    "csv-stringify": "^6.4.4",
    "dotenv": "^16.3.1",
    "fast-json-stringify": "^5.10.0",
    "fastify": "^5.4.0",
    "got": "^14.0.0",
    "helmet": "^7.1.0",
    "lodash": "^4.17.21",
    "ora": "^8.0.1",
    "p-queue": "^8.0.1",
    "pdfkit": "^0.14.0",
    "pino": "^8.17.2",
    "pino-pretty": "^10.3.1",
    "rate-limiter-flexible": "^4.0.1",
    "socket.io": "^4.7.4",
    "tough-cookie": "^4.1.3",
    "user-agents": "^1.1.0",
    "validator": "^13.11.0",
    "ws": "^8.18.3",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.4.1",
    "@playwright/test": "^1.40.0",
    "@types/better-sqlite3": "^7.6.8",
    "@types/lodash": "^4.14.202",
    "@types/node": "^20.10.6",
    "@types/pdfkit": "^0.12.12",
    "@types/tough-cookie": "^4.0.5",
    "@types/validator": "^13.11.7",
    "@types/ws": "^8.18.1",
    "@vitest/coverage-v8": "^3.2.4",
    "autocannon": "^7.14.0",
    "clinic": "^9.1.0",
    "concurrently": "^8.2.2",
    "husky": "^8.0.3",
    "jsdom": "^26.1.0",
    "lint-staged": "^15.2.0",
    "pkg": "^5.8.1",
    "prettier": "^3.6.2",
    "rimraf": "^5.0.5",
    "selenium-webdriver": "^4.33.0",
    "semantic-release": "^22.0.12",
    "tsup": "^8.0.1",
    "tsx": "^4.6.2",
    "typescript": "^5.3.3",
    "vitest": "^3.2.4"
  },
  "lint-staged": {
    "*.{ts,js}": [
      "biome format --write",
      "biome check --apply"
    ],
    "*.json": [
      "prettier --write"
    ],
    "*.md": [
      "prettier --write"
    ]
  },
  "tsup": {
    "entry": [
      "src/index.ts",
      "src/cli/index.ts",
      "src/web/cli.ts"
    ],
    "format": [
      "esm"
    ],
    "dts": true,
    "clean": true,
    "minify": true,
    "target": "node20"
  },
  "bin": {
    "job-dorker": "./dist/cli/index.js"
  }
}
